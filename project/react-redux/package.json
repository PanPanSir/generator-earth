{
    "name": "<%= name %>",
    "version": "<%= version %>",
    "private": true,
    "author": {
        "name": "<%= author %>",
        "email": "<%= email %>"
    },
    "repository": {
        "type": "git",
        "url": "http://gitlab.xxx.com/groups/<%= groupName %>/<%= name %>"
    },
    "group": "<%= groupName %>",
    "type": "<%= frameType %>",
    "keywords": [
        "earth",
        "react",
        "<%= name %>"
    ],
    "dependencies": {
        "classnames": "^2.2.5",
        "history": "^3.3.0",
        "json-server": "^0.12.0",
        "lm-loading": "^0.1.4",
        "pinkie-promise": "^2.0.1",
        "prop-types": "^15.5.10",
        "react": "15.6.1",
        "react-dom": "15.6.1",
        "react-redux": "^5.0.6",
        "react-router": "3.0.5",
        "redux": "^3.7.2",
        "redux-thunk": "^2.2.0",
        "whatwg-fetch": "2.0.3"
    },
    "scripts": {
        "start": "earth-scripts start",
        "build": "earth-scripts build",
        "update": "earth-scripts update",
        "test": "earth-scripts test --env=jsdom",
        "mock": "earth-scripts mock"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}"
        ],
        "setupFiles": [
            "<rootDir>/config/polyfills.js"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.js?(x)",
            "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
        ],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/node_modules/earth-scripts/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/earth-scripts/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "devDependencies": {
        "earth-scripts": "^0.2.0",
        "lm-flexible": "^0.1.1",
        "object-assign": "4.1.1",
        "promise": "8.0.1"
    },
    "proxy": "http://localhost:3001/"
}
