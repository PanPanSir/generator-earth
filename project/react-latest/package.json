{
    "name": "<%= name %>",
    "version": "<%= version %>",
    "private": true,
    "author": {
        "name": "<%= author %>",
        "email": "<%= email %>"
    },
    "repository": {
        "type": "git",
        "url": "http://gitlab.xxx.com/groups/<%= groupName %>/<%= name %>"
    },
    "group": "<%= groupName %>",
    "type": "<%= frameType %>",
    "keywords": [
        "earth",
        "react",
        "<%= name %>"
    ],
    "dependencies": {
        "classnames": "^2.2.5",
        "core-js": "^2.5.7",
        "history": "^4.7.2",
        "lm-loading": "^0.2.1",
        "prop-types": "^15.5.10",
        "query-string": "^5.0.1",
        "raf": "^3.4.0",
        "react": "16.4.0",
        "react-dom": "16.4.0",
        "react-router-dom": "^4.2.2"
    },
    "scripts": {
        "start": "earth-scripts start",
        "build": "earth-scripts build",
        "update": "earth-scripts update",
        "test": "earth-scripts test --env=jsdom",
        "mock": "earth-scripts mock"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}"
        ],
        "setupFiles": [
            "<rootDir>/node_modules/earth-scripts/config/polyfills.js"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.js?(x)",
            "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
        ],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/node_modules/earth-scripts/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/earth-scripts/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "devDependencies": {
        "earth-scripts": "^1.0.1"
    },
    "proxy": "http://localhost:3001/"
}
